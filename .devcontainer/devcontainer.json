{
	"name": "Go-Blockchain",
	"dockerComposeFile": [
		"../docker-compose.yml",
	],
	"service": "api",
	"workspaceFolder": "/go/src/app",
	"settings": {
		"go.toolsManagement.checkForUpdates": "off",
		"go.gopath": "/go",
		"go.inferGopath": true,
		"go.gocodeAutoBuild": true,
		"go.formatTool": "gofmt",
		"go.useLanguageServer": true,
		"editor.formatOnSave": false,
		"[go]": {
			"editor.formatOnSave": true
		}
	},
	"extensions": [
		"golang.go"
	],
}
