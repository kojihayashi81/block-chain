{
  "extends": "next/core-web-vitals",
  "settings": {
    "import/extensions": [
      ".js",
      ".jsx"
    ]
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "quotes": [
      2,
      "single"
    ]
  }
}